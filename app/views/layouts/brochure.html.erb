	<div id="header">
		<div class="page">
			<a href="/" class="logo">Book That App</a>
			<ul>
				<li class="<%= "active" if params[:controller] == "home" && params[:action] == "index" %>"><a href="/">Home</a></li>
				<li class="<%= "active" if params[:controller] == "login" %>"><a href="//www.bookthatapp.com/login">Sign In</a></li>
			</ul>
			<div class="clear"></div>
		</div>
	</div>

	<div id="page">
		<div class="top">
		</div>
		<div class="content">
			<%= yield %>
		</div>
		<div class="bottom"></div>
	</div>

	<div id="footer" class="page">
		<p>Copyright &copy; <%= current_year -%> Shopify Concierge. All rights reserved.</p>
		<ul>
			<li><a href="/">Home</a></li>
			<li><a href="//www.bookthatapp.com/login">Sign In</a></li>
		</ul>
		<div class="clear"></div>
	</div>

	<%= javascript_include_tag "//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" %>
	<%= javascript_include_tag "//#{request.env["HTTP_HOST"]}/javascripts/jquery.easing-1.3.pack.js" %>
	<%= javascript_include_tag "//#{request.env["HTTP_HOST"]}/javascripts/jquery.tools.brochure.js" %>
	<%= javascript_include_tag "//#{request.env["HTTP_HOST"]}/javascripts/jquery.mousewheel-3.0.2.pack.js" %>
	<%= javascript_include_tag "//#{request.env["HTTP_HOST"]}/javascripts/jquery.fancybox.js" %>
	<%= javascript_include_tag  "//#{request.env["HTTP_HOST"]}/javascripts/site.js" %>

    <%= yield :footer %>

