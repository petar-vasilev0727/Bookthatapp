{% if item.properties %}<p>
  {% for p in item.properties %}
  {% unless p.last == blank %}
  {{ p.first | replace: 'booking-start', 'Date' | replace: 'booking-time', 'Time' }}:
  {% if p.last contains '/uploads/' %}<a class="lightbox" href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
  {% else %}{{ p.last }}{% endif %}<br/>
  {% endunless %}
  {% endfor %}</p>
{% endif %}
