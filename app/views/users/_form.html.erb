<table id="product-form-table" class="form-table styled">
  <tbody>
  <tr>
    <td class="title">
      <%= f.label :email %>
    </td>
    <td>
      <%= f.email_field :email, autofocus: true %>
    </td>
  </tr>
  <tr>
    <td class="title">
      <%= f.label :name %>
    </td>
    <td>
      <%= f.text_field :name %>
    </td>
  </tr>
  <tr>
    <td class="title">
      <%= f.label :title %>
    </td>
    <td>
      <%= f.text_field :title %>
    </td>
  </tr>
  </tbody>
</table>


<div class="tab-footer clear" style="display: block; margin-top: 20px;">
  <div class="button-bar">
    <%= f.submit "Save" %>
    <%= link_to "Close", users_path %>
  </div>
</div>
