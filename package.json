{
  "name": "bookthatapp",
  "dependencies": {
    "fuelux": "^3.15.4",
    "fullcalendar": "^2.9.0",
    "fullcalendar-scheduler": "^1.3.2",
    "moment": "^2.13.0",
    "react": "^0.14.8",
    "react-addons-update": "^0.14.6",
    "react-big-calendar": "^0.10.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "rrule": "^2.1.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 5"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-2": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-incremental": "^3.0.1",
    "js-yaml": "^3.6.1",
    "babel-jest": "^9.0.0",
    "jest-cli": "*",
    "react-addons-test-utils": "~0.14.0"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "js.jsx"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/moment/",
      "<rootDir>/node_modules/babel-runtime",
      "<rootDir>/node_modules/react-big-calendar",
      "<rootDir>/lib/assets/javascripts/react-bootstrap-datetimepicker-noicon"
    ],
    "moduleNameMapper": {
      "react-bootstrap-datetimepicker-noicon": "<rootDir>/lib/assets/javascripts/react-bootstrap-datetimepicker-noicon"
    }
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
